{
  "name": "@esdoctor/template",
  "version": "0.1.0-alpha.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn build",
    "dev": "cross-env NODE_ENV=development vite",
    "typecheck": "tsc --noEmit",
    "build:web": "cross-env NODE_ENV=production vite build && yarn build:css",
    "build": "yarn build:web && tsup",
    "watch:css": "tailwindcss -i ./src/styles/global.css -o ./dist/global.css --watch",
    "build:css": "tailwindcss -m -i ./src/styles/global.css -o ./dist/global.css"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "echarts": "^6.0.0",
    "es-toolkit": "^1.39.10",
    "linkedom": "^0.18.9",
    "lucide-react": "^0.542.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-syntax-highlighter": "^15.6.6",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "traverse": "^0.6.11",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@esdoctor/types": "workspace:*",
    "@tailwindcss/cli": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@types/node": "^22",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-syntax-highlighter": "^15",
    "@types/react-virtualized": "^9",
    "@types/react-window": "^1",
    "@types/traverse": "^0",
    "@vitejs/plugin-react": "^5.0.2",
    "cross-env": "^10.0.0",
    "esbuild": "^0.25.9",
    "react-refresh": "^0.17.0",
    "tailwindcss": "^4.1.12",
    "tsup": "^8.5.0",
    "typescript": "5.9.2",
    "vite": "^7.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
