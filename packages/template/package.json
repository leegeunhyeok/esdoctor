{
  "name": "@esdoctor/template",
  "version": "0.1.0-alpha.2",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn build",
    "dev": "cross-env NODE_ENV=development concurrently \"rspack serve\" \"yarn watch:css\"",
    "doctor": "cross-env NODE_ENV=production RSDOCTOR=true rspack build",
    "build:web": "cross-env NODE_ENV=production rspack build && yarn build:css",
    "build": "yarn build:web && tsup",
    "watch:css": "tailwindcss -i ./src/styles/global.css -o ./dist/global.css --watch",
    "build:css": "tailwindcss -m -i ./src/styles/global.css -o ./dist/global.css"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-table": "^8.21.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "echarts": "^5.6.0",
    "es-toolkit": "^1.32.0",
    "linkedom": "^0.18.9",
    "lucide-react": "^0.474.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "traverse": "^0.6.11"
  },
  "devDependencies": {
    "@esdoctor/types": "0.1.0-alpha.2",
    "@rsdoctor/rspack-plugin": "^0.4.13",
    "@rspack/cli": "^1.2.2",
    "@rspack/core": "^1.2.2",
    "@rspack/plugin-react-refresh": "1.0.0",
    "@tailwindcss/cli": "^4.0.4",
    "@types/node": "^22.13.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15",
    "@types/traverse": "^0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.2",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^4.0.4",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
