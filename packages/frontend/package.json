    "@rspack/core": "^1.2.2",
    "@rspack/plugin-react-refresh": "1.0.0",
    "@tailwindcss/cli": "^4.0.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^9.1.2",
